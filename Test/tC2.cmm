int mod(int modx, int mody)
{
	return modx - modx / mody * mody;
}
		
int printHexDigit(int digit6)
{
	if( digit6 < 10)
		write(digit6);
	else
		write(-digit6);
	return 0;
}
		
int printHex(int number5)
{
	int a5[2];
	int j5 = 0;
	while (j5 < 2) {
		a5[j5] = mod(number5, 16);
		number5 = number5 / 16;
		j5 = j5 + 1;
	}
	j5 = 1;
	while (j5 >= 0) {
		printHexDigit(a5[j5]);
		j5 = j5 - 1;
	}
	return 0;
}
		
int prime_num(int num)
{
	int i = 2;
	int flag = 1;
	while (i < num) {
		if (mod(num,i) == 0)
			flag = 0;
			i = i + 1;
	}
	if (flag == 1)
		printHex(i);
	return 0;
}

int main()
{
	int n = 15;
	while (n < 20) {
		prime_num(n);
		n = n + 1;
	}
		
	return 0;
}
