int separate(int num)
{
	int flag = 0;
	int A[3];
	int j = 0;
	A[0] = num / 100;
	A[1] = num / 10 - 10 * A[0];
	A[2] = num - 100 * A[0] - 10 * A[1];
	if (A[0] == A[2]) {
		while (j < 12){
			if ((j * j - num) == 0 )
				flag = 1;
			j = j + 1;
		}
	}
	else flag = 0;
	if (flag == 1) return 1;
	else return 0;
}

int main()
{
	int i = 120;
	while (i < 130){
		if (separate(i) == 1)
			write(i);
		i = i + 1;
	}
	return 0;
}
